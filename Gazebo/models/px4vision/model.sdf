<?xml version="1.0" encoding="UTF-8"?>
<sdf version='1.9'>
  <model name='px4vision'>
    <gz:partition>px4</gz:partition>
    <pose>0 0 .24 0 0 0</pose>
    <self_collide>false</self_collide>
    <static>false</static>
    <link name='base_link'>
      <pose>0 0 0 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>1.5</mass>
        <inertia>
          <ixx>0.029125</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.029125</iyy>
          <iyz>0</iyz>
          <izz>0.055225</izz>
        </inertia>
      </inertial>
      <gravity>1</gravity>
      <velocity_decay />
      <collision name='base_link_inertia_collision'>
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.47 0.47 0.11</size>
          </box>
        </geometry>
        <surface>
          <contact>
            <ode>
              <min_depth>0.001</min_depth>
              <max_vel>0</max_vel>
            </ode>
          </contact>
          <friction>
            <ode/>
          </friction>
        </surface>
      </collision>
      <visual name='base_link_inertia_visual'>
        <pose>0 0 0 1.5707 0 0.01</pose>
        <geometry>
          <mesh>
            <scale>0.001 0.001 0.001</scale>
            <uri>model://px4vision/meshes/body.dae</uri>
          </mesh>
        </geometry>
        <material> <!-- DarkGrey-->
          <ambient>0.175 0.175 0.175 1.0</ambient>
          <diffuse>0.175 0.175 0.175 1.0</diffuse>
          <specular>0.175 0.175 0.175 1.0</specular>
        </material>
      </visual>
      <sensor name="air_pressure_sensor" type="air_pressure">
        <frame_id>base_link</frame_id>
        <always_on>1</always_on>
        <update_rate>50</update_rate>
        <plugin filename="gz-sim-air-pressure-system" name="gz::sim::systems::AirPressure" />
        <air_pressure>
          <topic>baro</topic>
          <pressure>
            <noise type="gaussian">
              <mean>0</mean>
              <stddev>0.01</stddev>
            </noise>
          </pressure>
        </air_pressure>
      </sensor>
      <sensor name="magnetometer_sensor" type="magnetometer">
        <frame_id>base_link</frame_id>
        <always_on>1</always_on>
        <update_rate>100</update_rate>
        <plugin filename="gz-sim-magnetometer-system" name="gz::sim::systems::Magnetometer" />
        <magnetometer>
          <!-- TODO: update to fix units and coordinate system when we move past Harmonic -->
          <!-- See https://github.com/gazebosim/gz-sim/pull/2460 -->
          <!-- 3mgauss RMS: NOTE: noise is in tesla but sensor reports data in gauss -->
          <!-- Noise modeled after IIS2MDC -->
          <topic>mag</topic>
          <x>
            <noise type="gaussian">
              <stddev>0.0001</stddev>
            </noise>
          </x>
          <y>
            <noise type="gaussian">
              <stddev>0.0001</stddev>
            </noise>
          </y>
          <z>
            <noise type="gaussian">
              <stddev>0.0001</stddev>
            </noise>
          </z>
        </magnetometer>
      </sensor>
      <sensor name="navsat_sensor" type="navsat">
        <frame_id>base_link</frame_id>
        <always_on>1</always_on>
        <update_rate>30</update_rate>
        <plugin filename="${NAVSAT_PLUGIN}" name="${NAVSAT_NAME}"/> <!-- different versions for gz and ign -->
        <topic>navsat</topic>
      </sensor>
      <sensor name="imu_sensor" type="imu">
        <frame_id>base_link</frame_id>
        <always_on>1</always_on>
        <update_rate>250</update_rate>
        <plugin filename="gz-sim-imu-system" name="gz::sim::systems::Imu"/>
        <imu>
          <topic>imu</topic>
          <angular_velocity>
            <x>
              <noise type="gaussian">
                <mean>0</mean>
                <stddev>0.00018665</stddev>
                <dynamic_bias_stddev>3.8785e-05</dynamic_bias_stddev>
                <dynamic_bias_correlation_time>1000</dynamic_bias_correlation_time>
              </noise>
            </x>
            <y>
              <noise type="gaussian">
                <mean>0</mean>
                <stddev>0.00018665</stddev>
                <dynamic_bias_stddev>3.8785e-05</dynamic_bias_stddev>
                <dynamic_bias_correlation_time>1000</dynamic_bias_correlation_time>
              </noise>
            </y>
            <z>
              <noise type="gaussian">
                <mean>0</mean>
                <stddev>0.00018665</stddev>
                <dynamic_bias_stddev>3.8785e-05</dynamic_bias_stddev>
                <dynamic_bias_correlation_time>1000</dynamic_bias_correlation_time>
              </noise>
            </z>
          </angular_velocity>
          <linear_acceleration>
            <x>
              <noise type="gaussian">
                <mean>0</mean>
                <stddev>0.00186</stddev>
                <dynamic_bias_stddev>0.006</dynamic_bias_stddev>
                <dynamic_bias_correlation_time>300</dynamic_bias_correlation_time>
              </noise>
            </x>
            <y>
              <noise type="gaussian">
                <mean>0</mean>
                <stddev>0.00186</stddev>
                <dynamic_bias_stddev>0.006</dynamic_bias_stddev>
                <dynamic_bias_correlation_time>300</dynamic_bias_correlation_time>
              </noise>
            </y>
            <z>
              <noise type="gaussian">
                <mean>0</mean>
                <stddev>0.00186</stddev>
                <dynamic_bias_stddev>0.006</dynamic_bias_stddev>
                <dynamic_bias_correlation_time>300</dynamic_bias_correlation_time>
              </noise>
            </z>
          </linear_acceleration>
        </imu>
      </sensor>
      <sensor name="gpu_lidar" type="gpu_lidar">
        <frame_id>base_link</frame_id>
        <pose> 0.07 0.032 -0.012 0 -0.4 0</pose>
        <always_on>1</always_on>
        <update_rate>30</update_rate>
        <ray>
          <topic>lidar</topic>
          <scan>
            <horizontal>
              <samples>640</samples>
              <resolution>1</resolution>
              <min_angle>-1.396263</min_angle>
              <max_angle>1.396263</max_angle>
            </horizontal>
            <vertical>
              <samples>640</samples>
              <resolution>1</resolution>
              <min_angle>-0.52</min_angle>
              <max_angle>0.26</max_angle>
            </vertical>
          </scan>
          <range>
            <min>0.08</min>
            <max>40.0</max>
            <resolution>0.01</resolution>
          </range>
        </ray>
        <!-- Optional visualization -->
        <visualize>true</visualize>
        <!-- Topic name -->
      </sensor>
      <sensor name="optical_flow" type="camera">
        <pose> 0.7 0.32 -4.0 0 1.5708 0 </pose> 
        <always_on>1</always_on>
        <update_rate>30</update_rate>
        <camera>
          <horizontal_fov>1.3962634</horizontal_fov>
          <topic>/flow</topic>
          <image>
            <width>64</width>
            <height>64</height>
            <format>L8</format>
          </image>
          <clip>
            <near>0.5</near>
            <far>20.0</far>
          </clip>
        </camera>
      </sensor>
