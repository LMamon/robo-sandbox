    <!-- RGB Camera Link -->
    <link name="rgb_camera_link">
      <pose> 0.09 -0.032 -0.012 0 0 0 </pose>  <!-- adjust as needed -->
      <!-- Minimal inertial block -->
      <inertial>
        <mass>0.01</mass>
        <inertia>
          <ixx>1e-6</ixx>
          <iyy>1e-6</iyy>
          <izz>1e-6</izz>
        </inertia>
      </inertial>

      <!-- Simple collision so physics doesn't explode -->
      <collision name="rgb_camera_collision">
        <geometry>
          <box><size>0.05 0.05 0.05</size></box>
        </geometry>
      </collision>

      <visual name="rgb_camera_visual">
        <geometry>
          <box><size>0.01 0.01 0.01</size></box>
        </geometry>
        <material>
          <ambient>0 0 1 1</ambient>
          <diffuse>0 0 1 1</diffuse>
        </material>
      </visual>
      <sensor name="rgb_camera" type="camera">
        <frame_id>base_link</frame_id>
        <always_on>1</always_on>
        <update_rate>30</update_rate>
        <!-- <plugin filename="gz-sim-camera-system" name="gz::sim::systems::Camera"/> -->
        <!-- <ros>
          <namespace>/px4vision</namespace>
          <remapping>image_raw:=rgb/image</remapping>
          <remapping>camera_info:=rgb/camera_info</remapping>
        </ros> -->
        <!-- <plugin filename="gz-sim-camera-system" name="gz::sim::systems::Camera"/> -->
        <!-- <plugin filename="libgz-sim-camera-system.so" name="gz::sim::systems::Camera"/> -->
        <camera>
          <topic>camera</topic>
          <horizontal_fov>1.5708</horizontal_fov>
          <image>
            <width>640</width>
            <height>480</height>
            <format>R8G8B8</format>
          </image>
          <clip>
            <near>0.02</near>
            <far>100.0</far>
          </clip>
        </camera>
      </sensor>
    </link>
    <joint name="rgb_camera_joint" type="fixed">
      <parent>base_link</parent>
      <child>rgb_camera_link</child>
    </joint>

    <!-- Depth Camera Link -->
    <link name="depth_camera_link">
      <gz_frame_id>base_link</gz_frame_id>
      <pose> 0.09 -0.012 -0.012 0 0 0 </pose>   <!-- same offset, adjust later -->

      <!-- Minimal inertial block -->
      <inertial>
        <mass>0.01</mass>
        <inertia>
          <ixx>1e-6</ixx>
          <iyy>1e-6</iyy>
          <izz>1e-6</izz>
        </inertia>
      </inertial>

      <!-- Simple collision -->
      <collision name="depth_camera_collision">
        <geometry>
          <box><size>0.05 0.05 0.05</size></box>
        </geometry>
      </collision>

      <!-- Visual -->
      <visual name="depth_camera_visual">
        <geometry>
          <box><size>0.01 0.01 0.01</size></box>
        </geometry>
        <material>
          <ambient>0 1 0 1</ambient>
          <diffuse>0 1 0 1</diffuse>
        </material>
      </visual>

      <sensor name="depth_camera" type="depth">
        <always_on>1</always_on>
        <update_rate>20</update_rate>
        <!-- <plugin filename="gz-sim-depth-camera-system" name="gz::sim::systems::DepthCamera"/> -->
        <depth_camera>
          <topic>depth_camera</topic>
          <horizontal_fov>1.0472</horizontal_fov>
          <image>
            <width>640</width>
            <height>480</height>
            <format>FLOAT32</format>
          </image>
          <clip>
            <near>0.02</near>
            <far>50.0</far>
          </clip>
        </depth_camera>
      </sensor>
    </link>

    <!-- Joint connecting depth camera to base_link -->
    <joint name="depth_camera_joint" type="fixed">
      <parent>base_link</parent>
      <child>depth_camera_link</child>
    </joint>

